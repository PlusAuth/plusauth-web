{
  "name": "@plusauth/web",
  "version": "0.3.0-beta.3",
  "description": "PlusAuth Web Util",
  "main": "dist/plusauth-web.js",
  "types": "dist/plusauth-web.d.ts",
  "module": "dist/plusauth-web.es.js",
  "repository": "https://github.com/PlusAuth/plusauth-web",
  "homepage": "https://github.com/PlusAuth/plusauth-web#readme",
  "author": {
    "name": "Ismail H. Ayaz",
    "email": "ismail.ayaz@ekinokssoftware.com"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "docs": "typedoc src/index.ts",
    "build": "vite build && api-extractor run",
    "lint": "eslint . --ext .ts --fix",
    "test": "mocha",
    "prepublishOnly": "npm run test && npm run build",
    "release": "cross-env GITHUB_TOKEN=$GITHUB_TOKEN release-it"
  },
  "dependencies": {
    "@simplewebauthn/browser": "^6.0.0",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@microsoft/api-extractor": "^7.31.2",
    "@release-it/conventional-changelog": "^5.1.0",
    "@simplewebauthn/typescript-types": "^6.0.0",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "chai": "^4.3.6",
    "core-js": "^3.25.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "nock": "^13.2.9",
    "release-it": "^15.4.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.15",
    "typescript": "4.8.3",
    "vite": "^3.1.3",
    "vite-plugin-dts": "^1.5.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        0,
        "always",
        100
      ],
      "scope-case": [
        0
      ]
    }
  },
  "lint-staged": {
    "*.ts": "npm run lint"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
