{
  "name": "@plusauth/web",
  "version": "0.1.0",
  "description": "PlusAuth Web Util",
  "main": "dist/plusauth-web.js",
  "types": "dist/plusauth-web.d.ts",
  "module": "dist/plusauth-web.esm.js",
  "repository": "https://github.com/PlusAuth/plusauth-web",
  "homepage": "https://github.com/PlusAuth/plusauth-web#readme",
  "author": {
    "name": "Ismail H. Ayaz",
    "email": "ismail.ayaz@ekinokssoftware.com"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "docs": "typedoc src/index.ts",
    "build": "bili && api-extractor run",
    "lint": "eslint . --ext .ts --fix",
    "test": "mocha",
    "prepublishOnly": "yarn test && yarn build",
    "release": "cross-env GITHUB_TOKEN=$GITHUB_TOKEN release-it"
  },
  "dependencies": {
    "whatwg-fetch": "^3.4.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@microsoft/api-extractor": "^7.11.2",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "bili": "^5.0.5",
    "chai": "^4.2.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "mocha": "^8.2.1",
    "nock": "^13.0.4",
    "release-it": "^14.2.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typescript": "4.0.5"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        0,
        "always",
        100
      ],
      "scope-case": [
        0
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": "yarn lint"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
