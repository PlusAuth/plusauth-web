{
  "name": "@plusauth/plusauth-web",
  "version": "0.0.4",
  "description": "PlusAuth Web Util",
  "main": "dist/plusauth-web.js",
  "types": "dist/plusauth-web.d.ts",
  "module": "dist/plusauth-web.esm.js",
  "repository": "https://github.com/PlusAuth/plusauth-web",
  "homepage": "https://github.com/PlusAuth/plusauth-web#readme",
  "author": {
    "name": "Ismail H. Ayaz",
    "email": "ismail.ayaz@ekinokssoftware.com"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "docs": "typedoc src/index.ts",
    "build": "bili && api-extractor run",
    "lint": "eslint . --ext .ts --fix",
    "test": "mocha",
    "prepublishOnly": "yarn test && yarn build",
    "release": "cross-env GITHUB_TOKEN=$GITHUB_TOKEN release-it"
  },
  "dependencies": {
    "whatwg-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@microsoft/api-extractor": "^7.9.2",
    "@release-it/conventional-changelog": "^1.1.4",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "bili": "^5.0.5",
    "chai": "^4.2.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^8.1.1",
    "nock": "^13.0.3",
    "release-it": "^13.6.6",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-node": "^8.10.2",
    "typedoc": "^0.17.8",
    "typescript": "3.9.7"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        0,
        "always",
        100
      ],
      "scope-case": [
        0
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
